{
  "name": "yet-another-react-bootstrap",
  "version": "0.1.0",
  "description": "React bootstrap application. React, webpack, ES6/7, karma, mocha, chai, sinon, enzyme.",
  "private": true,
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3 <4"
  },
  "main": "",
  "scripts": {
    "clean": "npm run clean:reports && rimraf build",
    "clean:reports": "rimraf reports",
    "build": "npm run clean && NODE_ENV=production webpack --progress -p",
    "start": "npm run clean && NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "test": "npm run clean:reports && NODE_ENV=test karma start --single-run --no-auto-watch",
    "test:watch": "NODE_ENV=test karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aeldar/yet-another-react-bootstrap.git"
  },
  "keywords": [
    "react",
    "webpack",
    "bootstrap"
  ],
  "author": "Eldar Aliyev <eldar.aliyev8@gmail.com>",
  "license": "MIT",
  "bugs": {
